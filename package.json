    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-svelte3": "^4.0.0",
    "husky": "^8.0.1",
    "lint-staged": "^12.4.1",
    "postcss": "^8.4.13",
    "postcss-load-config": "^3.1.4",
    "prettier": "^2.6.2",
    "prettier-plugin-svelte": "^2.7.0",
    "sharp": "^0.30.4",
    "stylus": "^0.55.0",
    "svelte": "^3.48.0",
    "svelte-preprocess": "^4.10.6",
    "tailwindcss": "^3.0.24",
    "vite": "^2.9.9"
  },
  "dependencies": {
    "scrollreveal": "^4.0.9"
  },
  "lint-staged": {
